field(sid='CourseTitle_INDEX_')
    scrollhoriz never
    scrollvert never
    value <%= course.Title %>
    itemlocation
        x 110
        y <%= offsetTitle %>
        width 445
    previous CourseNumber_INDEX_
    format
        datatype string
        presentation
            casetype upper
        constraints
            length
                min 0
                max 2147483647
            mandatory on
    next Backlog_INDEX_

field(sid='CourseNotes_INDEX_')
    scrollhoriz never
    scrollvert fixed
    value Min/Max: <%= course.Min %>/<%= course.Max %>, Days/Hrs: <%= course.Days %>/<%= course.Hours %>, MDS: <%= course.MDS %>\r<%= notes %>
    itemlocation
        x 573
        y <%= offset %>
        width 380
        height 70
    previous Month3_INDEX_
    format
        datatype string
        presentation