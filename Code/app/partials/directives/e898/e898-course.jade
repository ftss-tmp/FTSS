field(sid='PDS_INDEX_')
    scrollhoriz never
    scrollvert never
    value <%= course.PDS %>
    itemlocation
        x 57
        y <%= offset %>
        width 42
    previous CAFMCL_INDEX_
    format
        datatype string
        presentation
            casetype upper
        constraints
            mandatory on
    next CourseNumber

check(sid='CAFMCL_INDEX_')
    itemlocation
        x 18
        y <%= offset %>
    value <%= cafmcl %>
    next PDS_INDEX_
    previous FTD_INDEX_

field(sid='CourseNumber_INDEX_')
    scrollhoriz never
    scrollvert never
    value <%= course.Number %>
    itemlocation
        x 110
        y <%= offset %>
        width 180
    previous PDS_INDEX_
    format
        datatype string
        presentation
            casetype upper
        constraints
            length
                min 0
                max 20
            mandatory on
    next CourseTitle_INDEX_

field(sid='Backlog_INDEX_')
    scrollhoriz never
    scrollvert never
    value <%= seatCount %>
    itemlocation
        x 302
        y <%= offset %>
        width 59
    previous CourseTitle_INDEX_
    format
        datatype integer
        constraints
            range
                min 0.0
                max 999.0
            mandatory on
        presentation
    justify center
    next Month1_INDEX_

field(sid='Month1_INDEX_')
    scrollhoriz never
    scrollvert never
